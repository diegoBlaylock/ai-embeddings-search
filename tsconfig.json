{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "moduleResolution": "nodenext",
    "noImplicitAny": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "outDir": "dist"
  },
  "include": [
    "./src/ts/**/*.ts"
, "src/ts/data/scrapper"  ]
}